		{
			"name": "Get Camião Não Existente",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Tentativa de Listagem de Camioes\", function () {\r",
							"pm.expect(pm.response.code).to.be.oneOf([200,404,500]);});\r",
							"pm.test(\"Matricula nao existe\", function () {\r",
							"    var camiao = pm.response.text();\r",
							"    pm.expect(camiao).to.not.include(\"BA-12-NA\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
