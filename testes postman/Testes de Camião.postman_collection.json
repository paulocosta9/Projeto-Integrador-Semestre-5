				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Criar Camião\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200 ,201]);\r",
							"});\r",
							"\r",
							"pm.test(\"Camiao criado\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.matricula).to.eql(\"BA-12-NA\");\r",
							"    pm.globals.set(\"id\",jsonData.id);\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
